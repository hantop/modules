<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="FlowBidOrderMapper">

    <insert id="add" parameterType="com.fenlibao.p2p.model.trade.entity.order.T6508" >
        INSERT INTO s65.t6508 (F01,F02,F03) VALUES (#{F01},#{F02},#{F03})
    </insert>
    
    <select id="get" resultType="com.fenlibao.p2p.model.trade.entity.order.T6508">
        select F01,F02,F03 from s65.t6508 where F01 = #{id}
    </select>
    
</mapper>