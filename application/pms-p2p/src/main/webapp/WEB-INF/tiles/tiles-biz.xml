<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
        "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
        "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
    <definition extends="ancestor" name="biz/*">
        <put-attribute name="body" value="/WEB-INF/page/biz/{1}.jsp"/>
        <put-attribute name="menu" value="/WEB-INF/page/template/menu/menu-biz.jsp"/>
    </definition>

    <definition extends="ancestor" name="biz/*/*">
        <put-attribute name="body" value="/WEB-INF/page/biz/{1}/{2}.jsp"/>
        <put-attribute name="menu" value="/WEB-INF/page/template/menu/menu-biz.jsp"/>
    </definition>
</tiles-definitions>